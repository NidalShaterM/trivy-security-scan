{
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-eval": "error",
    "no-implied-eval": "error",
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }]  // Change 'error' to 'warn'
  }
}
